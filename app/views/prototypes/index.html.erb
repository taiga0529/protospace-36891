<main class="main">
  <div class="inner">
    <%# ログインしているときは以下を表示する %>
      <% if user_signed_in? %>
      <div class="greeting">
      <span>
      <%="こんにちは、"%>
      <%= current_user.name %>
         <%= link_to  "さん", user_path(current_user.id), class: :greeting__link%>
      </span>
      </div> 
      <% end %>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
      <%= render partial: 'prototype', collection: @prototypes %>
    </div>
  </div>
</main>
